{
    "id": "osr",
    "title": "Outcast Silver Raiders",
    "url": "https://github.com/MilasBaumgarten/foundryvtt_osr/",
    "manifest": "https://raw.githubusercontent.com/MilasBaumgarten/foundryvtt_osr/refs/heads/main/system.json",
    "download": "https://github.com/MilasBaumgarten/foundryvtt_osr/releases/download/latest/osr.zip",
    "license": "LICENSE.md",
    "description": "An implementation of the Outcast Silver Raiders system.",
    "version": "0.1.4",
    "compatibility": {
      "minimum": 12,
      "verified": 12
    },
    "authors": [{
      "name": "Milas Baumgarten",
      "email": "milasbaumgarten@gmail.com",
      "discord": "gwydiondesert"
    }],
    "esmodules": ["osr.mjs"],
    "styles": ["styles/system-styles.css"],
    "packs": [{
      "name": "monsters",
      "label": "My Monsters",
      "type": "Actor"
    },
    {
      "name": "items",
      "label": "My Items",
      "type": "Item"
    }],
    "languages": [{
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }],
    "documentTypes": {
      "Actor": {
        "pc": { "htmlFields": ["background.biography", "classFeatures"] },
        "npc": { "htmlFields": ["features", "notes"] }
      },
      "Item": {
        "weapon": {},
        "spell": {},
        "generic": {}
      }
    },
    "socket": false,
    "initiative": "1d10",
    "grid": {
      "distance": 5,
      "units": "ft"
    },
    "primaryTokenAttribute": "resources.health"
  }